{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": ["warn"],
        "no-useless-escape": ["warn"],
        "no-unreachable": ["warn"],
        "no-unsafe-finally": ["off"],
        "no-empty": ["off"],
        "no-prototype-builtins": ["off"],
        "no-control-regex": ["off"]
    }
}
